{
  "name": "ismailivolunteer",
  "version": "0.1.0",
  "description": "Ismaili Volunteer Listings",
  "main": "none",
  "private": true,
  "scripts": {
    "scss": "./node_modules/.bin/node-sass -r ./styles -o ./static/styles --include-path ./node_modules --output-style compressed",
    "styles": "npm run scss && ./node_modules/.bin/postcss static/styles/*.css --replace",
    "images": "cp -r images static",
    "error": "./node_modules/.bin/rollup -c rollup.config.js scripts/error.js -o static/scripts/error.js -n error",
	"form_submit": "./node_modules/.bin/rollup -c rollup.config.js scripts/form_submit.js -o static/scripts/form_submit.js -n form_submit",
    "index_jumbotron": "./node_modules/.bin/rollup -c rollup.config.js scripts/index_jumbotron.js -o static/scripts/index_jumbotron.js -n index_jumbotron",
    "old_browsers": "./node_modules/.bin/rollup -c rollup.config.js scripts/old_browsers.js -o static/scripts/old_browsers.js -n old_browsers",
	"smooth": "./node_modules/.bin/rollup -c rollup.config.js scripts/smooth.js -o static/scripts/smooth.js -n smooth",
    "modernizr": "./node_modules/.bin/modernizr -c modernizr-config.json -d static/scripts",
    "scripts": "npm run error && npm run form_submit && npm run index_jumbotron && npm run old_browsers && npm run smooth && npm run modernizr",
    "fontawesome": "cp -r node_modules/font-awesome/fonts/ fonts",
    "fonts": "npm run fontawesome && cp -r fonts static",
    "public": "cp -r public/ static",
    "static": "rm -rf ./static && npm run styles && npm run public && npm run images && npm run scripts && npm run fonts",
    "minify_html": "./node_modules/.bin/html-minifier --collapse-whitespace --remove-comments --minify-js --minify-css --file-ext html --input-dir ./dist --output-dir ./dist",
    "minify_scripts": "find . -path '*dist/scripts*' -name '*.js' -exec ./node_modules/.bin/uglifyjs '{}' -o '{}' ';'",
    "minify": "npm run minify_html && npm run minify_scripts",
    "compile": "npm run static && npm run public && hugo && npm run minify",
    "netlify": "npm run static && npm run public && hugo_0.19 && npm run minify",
    "phantom_headless": "chrome --remote-debugging-port=9222",
    "phantom_prepare": "rm -rf content/phantom && cp -r content/listings content/phantom",
    "phantom": "npm run phantom_headless && node phantom/runner.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ismailivolunteer/ismailivolunteer.git"
  },
  "author": "Alizain Feerasta",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ismailivolunteer/ismailivolunteer/issues"
  },
  "homepage": "https://github.com/ismailivolunteer/ismailivolunteer#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.0",
    "chrome-remote-interface": "^0.22.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-compat": "^1.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.1",
    "html-minifier": "^3.4.2",
    "minimist": "^1.2.0",
    "node-sass": "^4.4.0",
    "postcss": "^5.2.17",
    "postcss-cli": "^3.1.1",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^3.2.2",
    "uglify-js": "^2.8.22"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "bootstrap": "4.0.0-beta.2",
    "font-awesome": "latest",
    "jquery": "^3.1.1",
    "jump.js": "^1.0.2",
    "modernizr": "^3.4.0",
    "parsleyjs": "^2.6.2",
    "phantomjs-prebuilt": "^2.1.14"
  }
}
